{
  "name": "apm-vite-web",
  "version": "4.4.0-alpha.1",
  "license": "MIT",
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "test": "jest",
    "test:coverage": "npm run test --watchAll=false --collectCoverageFrom=src/**/*.ts* --coverage",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|json|css|md|mdx|html)\"",
    "lint": "eslint . --cache-location node_modules/.cache/eslint --fix",
    "typecheck": "tsc --noEmit",
    "validate": "npm-run-all --parallel format lint typecheck build",
    "cy:open-local": "cypress open --e2e --browser firefox --config-file cypress/config/local.config.ts",
    "cy:run-local": "cypress run --e2e --browser firefox --config-file cypress/config/local.config.ts",
    "cy:open-ct": "cypress open --component --browser firefox --config-file cypress/config/local.config.ts",
    "cy:run-ct": "cypress run --component --browser firefox --config-file cypress/config/local.config.ts",
    "cy:run-ct-fast": "npm run cy:run-ct --config video=false screenshot=false cypress/config/local.config.ts"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.4.0",
    "@bugsnag/js": "^8.4.0",
    "@bugsnag/plugin-react": "^8.4.0",
    "@devexpress/dx-react-chart": "^4.0.11",
    "@devexpress/dx-react-chart-material-ui": "^4.0.11",
    "@devexpress/dx-react-core": "^4.0.11",
    "@devexpress/dx-react-grid": "^4.0.11",
    "@devexpress/dx-react-grid-material-ui": "^4.0.11",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.6",
    "@fortawesome/fontawesome-svg-core": "^7.0.1",
    "@fortawesome/free-regular-svg-icons": "^7.0.1",
    "@fortawesome/free-solid-svg-icons": "^7.0.1",
    "@fortawesome/react-fontawesome": "^3.0.2",
    "@hello-pangea/dnd": "^18.0.1",
    "@mui/icons-material": "^6.5.0",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.0",
    "@mui/x-tree-view": "^8.11.0",
    "@orbit/coordinator": "^0.17.0",
    "@orbit/core": "^0.17.0",
    "@orbit/indexeddb": "^0.17.2",
    "@orbit/indexeddb-bucket": "^0.17.0",
    "@orbit/jsonapi": "^0.17.1",
    "@orbit/memory": "^0.17.2",
    "@orbit/records": "^0.17.0",
    "@orbit/serializers": "^0.17.0",
    "@redux-devtools/extension": "^3.3.0",
    "@types/path-browserify": "^1.0.3",
    "@uiw/react-color-colorful": "^2.8.0",
    "@xmldom/xmldom": "^0.8.11",
    "@wavesurfer/react": "^1.0.11",
    "array-move": "^4.0.0",
    "audio-recorder-polyfill": "^0.4.1",
    "axios": "^1.11.0",
    "audiobuffer-to-wav": "^1.0.0",
    "browser-image-compression": "^2.0.2",
    "electron-updater": "^6.3.9",
    "fingerprintjs2": "^2.1.4",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "luxon": "^3.7.1",
    "mui-language-picker": "^2.5.2",
    "path-browserify": "^1.0.1",
    "react": "18.3.1",
    "react-datasheet": "^1.4.12",
    "react-dom": "18.3.1",
    "react-draggable": "^4.5.0",
    "react-file-drop": "^3.1.6",
    "react-icons": "^5.5.0",
    "react-lifecycles-compat": "^3.0.4",
    "react-localization": "^1.0.19",
    "react-markdown": "^10.1.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.8.2",
    "react-select": "^5.10.2",
    "react-string-replace": "^1.1.1",
    "react-style-proptype": "^3.2.2",
    "redux": "^5.0.1",
    "redux-thunk": "^3.1.0",
    "remark-gfm": "^4.0.1",
    "styled-components": "^6.1.19",
    "url-parse": "^1.5.10",
    "wavesurfer.js": "^7.10.1",
    "xpath": "^0.0.34"
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "exclude": [
      "node_modules",
      "dist",
      "build",
      "out"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@bahmutov/cy-api": "^2.2.6",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.3",
    "@cypress/grep": "4.1.0",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@faker-js/faker": "9.8.0",
    "@testing-library/cypress": "10.0.3",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "5.17.0",
    "@testing-library/react": "15.0.7",
    "@testing-library/user-event": "14.6.1",
    "@types/audiobuffer-to-wav": "^1.0.4",
    "@types/fingerprintjs2": "^2.0.0",
    "@types/jest": "29.5.14",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.17.7",
    "@types/luxon": "^3.7.1",
    "@types/node": "22.15.31",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@types/react-redux": "^7.1.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/url-parse": "^1.4.11",
    "@types/wavesurfer.js": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "8.34.0",
    "@typescript-eslint/parser": "8.34.0",
    "@vitejs/plugin-react": "4.5.2",
    "cypress": "^14.5.4",
    "cypress-data-session": "^2.8.5",
    "cypress-map": "^1.40.0",
    "dotenv": "^16.4.5",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.5",
    "eslint-import-resolver-typescript": "4.4.3",
    "eslint-plugin-cypress": "3.6.0",
    "eslint-plugin-implicit-dependencies": "1.1.1",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-no-only-tests": "3.3.0",
    "eslint-plugin-prettier": "5.4.1",
    "husky": "4.3.8",
    "identity-obj-proxy": "3.0.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "lint-staged": "16.1.0",
    "lodash": "^4.17.21",
    "npm-run-all2": "8.0.4",
    "prettier": "3.5.3",
    "start-server-and-test": "2.0.12",
    "ts-jest": "29.2.5",
    "ts-node": "10.9.2",
    "typescript": "5.8.3",
    "vite": "^6.0.0"
  },
  "author": {
    "name": "SIL Global",
    "email": "info@sil.org",
    "url": "http://sil.org"
  },
  "build": {
    "appId": "sil.lsdev.audio-project-manager",
    "productName": "Audio Project Manager",
    "copyright": "Copyright Â© 2020-2025 ${author}",
    "win": {
      "icon": "public/icon.png"
    },
    "linux": {
      "icon": "debian/audio-project-manager.png",
      "desktop": "debian/audio-project-manager.desktop",
      "target": [
        "deb"
      ]
    },
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "public/icon.png",
      "hardenedRuntime": true,
      "entitlements": "env-config/entitlements.mac.plist",
      "entitlementsInherit": "env-config/entitlements.mac.plist",
      "gatekeeperAssess": false,
      "extendInfo": {
        "NSMicrophoneUsageDescription": "Please give us access to your microphone"
      }
    },
    "dmg": {
      "sign": false
    },
    "afterSign": "env-config/notarize.js",
    "directories": {
      "buildResources": "assets"
    },
    "extraFiles": [
      {
        "from": "public/localization",
        "to": "localization",
        "filter": "*.json"
      },
      {
        "from": "public/help",
        "to": "help",
        "filter": "*.chm"
      },
      {
        "from": "public/help",
        "to": "help",
        "filter": "*.xlsx"
      },
      {
        "from": "public/script",
        "to": "resources",
        "filter": [
          "resetData.*",
          "*.py",
          "*.aup3"
        ]
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run typecheck && npm run test --watchAll=false && lint-staged"
    }
  },
  "volta": {
    "node": "22.19.0"
  }
}
